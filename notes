



numberOfLikes = () => {
    let currentNumber = 0
    if (this.state.toggle = true)
        this.state.likes + 1
    else if (this.state.toggle = false)
        this.state.likes - 1
        

    this.setState({
        likes: currentNumber
    })
}

numberOfLikes = () => {
    if (this.state.likes === 0)
        this.setState({ likes: 1 }
    else 
        this.setState({ likes: 0 })
    }
}